{
  "name": "exprestive",
  "version": "0.8.0",
  "description": "Expressive RESTful express router",
  "author": "Alex David",
  "bugs": {
    "url": "https://github.com/alexdavid/exprestive/issues"
  },
  "dependencies": {
    "callsite": "^1.0.0",
    "camel-case": "^1.1.1",
    "express": "^4.11.1",
    "inflection": "^1.5.3",
    "lodash": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coffee-script": "^1.8.0",
    "coffeelint": "^1.10.1",
    "cucumber": "^0.4.7",
    "gulp": "^3.8.10",
    "gulp-coffee": "^2.2.0",
    "mkdirp": "^0.5.0",
    "mycha": "^0.6.2",
    "npm-check-updates": "^1.5.1",
    "portfinder": "^0.3.0",
    "request": "^2.51.0",
    "rewire": "^2.1.4",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0",
    "tmp": "0.0.24"
  },
  "homepage": "https://github.com/alexdavid/exprestive",
  "license": "MIT",
  "main": "lib/middleware.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/alexdavid/exprestive.git"
  },
  "scripts": {
    "build": "gulp",
    "feature-tests": "./node_modules/.bin/cucumber-js test/feature --format=progress",
    "lint": "find . -name '*.coffee' -not -path './node_modules/*' | xargs coffeelint -f coffeelint.json5",
    "postpublish": "git push && git push --tags",
    "prefeature-tests": "npm run build",
    "prepublish": "npm run build",
    "preversion": "npm test",
    "test": "npm run lint && npm run unit-tests && npm run feature-tests",
    "unit-tests": "./node_modules/.bin/mycha run --reporter spec test/spec",
    "update": "npm-check-updates -u && rm -rf node_modules && npm install && npm test",
    "update-check": "npm-check-updates"
  }
}
