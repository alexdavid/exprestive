  },
  "devDependencies": {
    "async": "^1.4.0",
    "chai": "^3.0.0",
    "coffee-script": "^1.8.0",
    "coffeelint": "^1.10.1",
    "connect": "^3.4.0",
    "cucumber": "^0.9.2",
    "david": "^7.0.0",
    "fs-extra": "^0.26.2",
    "gulp": "^3.8.10",
    "gulp-coffee": "^2.2.0",
    "mycha": "^0.9.1",
    "portfinder": "^0.4.0",
    "request": "^2.51.0",
    "rewire": "^2.1.4",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "tmp": "0.0.28"
  },
  "homepage": "https://github.com/alexdavid/exprestive",
  "license": "MIT",
  "main": "lib/middleware.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/alexdavid/exprestive.git"
  },
  "scripts": {
    "build": "gulp",
    "feature-tests": "cucumber-js test/feature --format=progress",
    "lint": "find . -name '*.coffee' -not -path './node_modules/*' | xargs coffeelint -f coffeelint.json5",
    "postpublish": "git push && git push --tags",
    "prefeature-tests": "npm run build",
    "prepublish": "npm run build",
    "preversion": "npm test",
    "test": "npm run lint && npm run unit-tests && npm run feature-tests",
